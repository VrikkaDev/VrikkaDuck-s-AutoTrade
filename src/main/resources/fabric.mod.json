{
  "schemaVersion": 1,
  "id": "vrikkaducks-autotrade",
  "version": "${version}",

  "name": "VrikkaDuck's Autotrade",
  "description": "Allows you to auto trade with villagers",

  "authors": [
    "VrikkaDuck",
    "TheMasterFox"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/vrikkaducks-autotrade/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.VrikkaDuck.AutoTrade.AutoTrade"
    ],
    "modmenu": [
      "net.VrikkaDuck.AutoTrade.compat.modmenu.ModMenuImpl"
    ]
  },
  "mixins": [
    "vrikkaducks-autotrade.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.20",
    "java": ">=17",
    "malilib": ">=0.12.0"
  }
}
